{{ form_start(form) }}

    <table class="table">
        <tbody>
            <tr>
                <th>Title</th> 
                <td>{{ form_widget(form.title) }}</td>

            <tr>
            <tr>
                <th>Content</th>
                <td>{{ form_widget(form.content) }}</td>
            <tr> 

            <tr>
                <th>ImageUrl</th>
                <td>{{ form_widget(form.imageUrl) }}</td>
            <tr>

            <tr>
                <th>Author</th>
                <td>{{ form_widget(form.author) }}</td>
            <tr>

            <tr>
                <th>Categories</th>
                <td>{{ form_widget(form.categories) }}</td>
            <tr>
        </tbody>
    </table> 

    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
